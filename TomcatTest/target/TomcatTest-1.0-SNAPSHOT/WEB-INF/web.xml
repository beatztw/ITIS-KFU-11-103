<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>com.itis.servlets.ProfileServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.itis.servlets.UserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CarsServlet</servlet-name>
        <servlet-class>com.itis.servlets.CarsServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CatalogServlet</servlet-name>
        <servlet-class>com.itis.servlets.CatalogServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CarsServlet</servlet-name>
        <url-pattern>/cars</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CatalogServlet</servlet-name>
        <url-pattern>/catalog</url-pattern>
    </servlet-mapping>



</web-app>